{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "albertcht/invisible-recaptcha": "^1.8",
        "aws/aws-sdk-php-laravel": "^3.1",
        "barryvdh/laravel-debugbar": "^3.0",
        "creativeorange/gravatar": "~1.0",
        "curl/curl": "^1.9",
        "darkaonline/l5-swagger": "5.6.*",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.9",
        "google/recaptcha": "^1.1",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.4",
        "intouch/laravel-newrelic": "~2.0",
        "itsgoingd/clockwork": "^3.0",
        "jaybizzle/laravel-crawler-detect": "1.*",
        "jdavidbakr/signed-s3-filesystem": "^1.0",
        "jeremykenedy/laravel-exception-notifier": "^1.1",
        "jeremykenedy/laravel-https": "^1.1",
        "jeremykenedy/laravel-logger": "^2.0",
        "jeremykenedy/laravel-roles": "^1.2",
        "jeremykenedy/uuid": "^3.1",
        "josiasmontag/laravel-localization": "^0.4.2",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maennchen/zipstream-php": "^0.5.1",
        "paquettg/php-html-parser": "^1.7",
        "parsedown/laravel": "^1.0",
        "pusher/pusher-php-server": "^3.0",
        "soapbox/laravel-formatter": "^2.0",
        "socialiteproviders/37signals": "^3.0",
        "socialiteproviders/generators": "^2.0",
        "socialiteproviders/google": "^3.0",
        "socialiteproviders/instagram": "^3.0",
        "socialiteproviders/reddit": "^3.0",
        "socialiteproviders/twitch": "^3.0",
        "socialiteproviders/youtube": "^3.0",
        "spatie/array-to-xml": "^2.7",
        "spatie/laravel-fractal": "^5.2",
        "spatie/laravel-server-side-rendering": "^0.2.2",
        "spatie/laravel-tail": "^3.1",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/yaml": "^4.1",
        "torann/geoip": "^1.0",
        "zircote/swagger-php": "^3.0@dev",
		"tom-lingham/searchy" : "2.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
		"files": [
			"app/Http/Helpers/Helpers.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
